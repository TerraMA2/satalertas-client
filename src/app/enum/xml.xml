<featureType>
    <name>test_new</name>
    <nativeName>test_new</nativeName>
    <title>test_new</title>
    <srs>EPSG:4326</srs>
    <projectionPolicy>FORCE_DECLARED</projectionPolicy>
    <enabled>true</enabled>
    <metadata>
        <entry key="cachingEnabled">false</entry>
        <entry key="JDBC_VIRTUAL_TABLE">
            <virtualTable>
                <name>test_new</name>
                <sql>SELECT main_table.*, secondary_table.gid
                    FROM public.apv_app_cardeter_35 AS main_table
                    ,public.de_biomas_mt secondary_table
                    WHERE
                    ST_Intersects(ST_CollectionExtract(intersection_geom, 3), secondary_table.geom)</sql>
                <escapeSql>false</escapeSql>
                <keyColumn>gid</keyColumn>
                <geometry>
                    <name>intersection_geom</name>
                    <type>Polygon</type>
                    <srid>4326</srid>
                </geometry>
            </virtualTable>
        </entry>
    </metadata>
    <maxFeatures>0</maxFeatures>
    <numDecimals>0</numDecimals>
</featureType>
